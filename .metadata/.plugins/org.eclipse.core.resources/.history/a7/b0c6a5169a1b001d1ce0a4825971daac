package random;

public class palindromeNum {

	public static void main(String[] args) {

  
	     int num=1234321;
	     int rem;
	     int rev=0;
	     
	     while(num!=0) {
	     rem=num%10;
	     rev=rev*10+rem;
	     num=num/10;
	      }
	     System.out.println(rev);
		if(num==rev) {
			System.out.println("num is palindrome");
		}
		else {
		System.out.println("num is not palindrome");
		}
		
		
		
		
		
		
	}

}
