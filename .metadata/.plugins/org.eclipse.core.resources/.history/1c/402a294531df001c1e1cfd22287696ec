package abstractclass;

public abstract  class MumbaiIndians {

	 int a=10;    /// cannot call non static global variable
	static String name= "vishal";
	
	
	
	public static void main(String[] args) {

		
		System.out.println("my name is "+name);
			
	}		
		
		
	

	public void Hitman()
	{
		System.out.println("no 1 T20 batter in the world");
	}
	 public abstact void bumrah() {
		 System.out.println("one of the best bowler in the world");
	 }
	 
	public abstract  void ishan ();
	
	abstract void surya ();
	
	public abstract void devid();
}
