package inheritance;

public class Son extends Mother {
   
	public void mobile () {
		System.out.println(" sons mobile");}
	
	public void laptop (){
		System.out.println(" sons laptop");
	}	
	public static void earphones() {
		System.out.println(" earphones ");
	}
	
	}

