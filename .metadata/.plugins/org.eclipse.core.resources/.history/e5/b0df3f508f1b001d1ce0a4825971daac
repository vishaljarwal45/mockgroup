package random;

public class fibinacci {

	public static void main(String[] args) {
//a,b,c

		int no1=0;
		int no2=1;
		
		System.out.print(no1+" "+no2);
		
		for(int i=1;i<=10;i++) {
			
			int c=no1+no2;
			System.out.println(" "+c);
			int no11=no2;
			int no21=c;
			
		}}

		
		
		
		
		
		
		
	}


