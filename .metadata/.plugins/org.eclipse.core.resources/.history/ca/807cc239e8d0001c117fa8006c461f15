package practice;

public class PrcSwitchCase {

	public static void main(String[] args) {
		
		String="monday";
		
	switch (day) {
	case monday:System.out.println("welcome its first day of the week ");
		break;

	case tuesday:System.out.println("this is second day of the week ");
		break;
	
	case wednesday:System.out.println("this is third day of the week");	
	   break;
	   
	case thursday:System.out.println("this is fourth day of the week ");
	  break;
	  
	case friday:System.out.println("this is fifth day of the week"); 
	  break;
	  
	case saturday:System.out.println("this is sixth day of the week");
	  break;
	  
	case sunday:System.out.println("this is seventh day of the week");
	 break;
	}
		
		
		
	}
		
	}


